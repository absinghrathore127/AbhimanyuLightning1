<aura:component >

    <aura:attribute name="expenses" type="Account[]"/>
<aura:attribute name="mouseStart" type="string" />
    <aura:attribute name="oldWidth" type="string" />
    
    <div class="slds-card slds-p-top--medium">
        <header class="slds-card__header">
            <h3 class="slds-text-heading--small">Accounts</h3>
        </header>
        
        <section class="slds-card__body">
            <div id="list" class="row">
                    <c:AccountItem expenses="{!v.expenses}"/>
            </div>
        </section>
    </div>

</aura:component>